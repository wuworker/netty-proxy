# Application Listeners
org.springframework.context.ApplicationListener=\
  org.springframework.boot.context.ApplicationPidFileWriter
# Failure Analyzers
org.springframework.boot.diagnostics.FailureAnalyzer=\
  com.wxl.proxy.exception.BeanConfigFailureAnalyzer

org.springframework.boot.autoconfigure.EnableAutoConfiguration=\
  com.wxl.proxy.autoconfig.ProxyServerAutoConfiguration,\
  com.wxl.proxy.autoconfig.HttpProxyAutoConfiguration,\
  com.wxl.proxy.autoconfig.TcpProxyAutoConfiguration
